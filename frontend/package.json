    "@ethercorps/sveltekit-og": "^4.2.1",
    "@lezer/highlight": "^1.2.3",
    "@uiw/codemirror-theme-androidstudio": "^4.25.4",
    "@uiw/codemirror-theme-aura": "^4.25.4",
    "@uiw/codemirror-theme-basic": "^4.25.4",
    "axios": "^1.13.2",
    "codemirror": "^6.0.2",
